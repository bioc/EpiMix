CancerSite = "BRCA"
EpiMixResults = readRDS(paste0("/labs/gevaertlab/TCGA_methylation/Results/TCGA_", CancerSite, "/EpiMix_Results_regular.rds"))
FunctionalPairs <- EpiMixResults$FunctionalPairs
FunctionalPairs <- filterDual(FunctionalPairs)
FunctionalPairs <- addEnsemblID(FunctionalPairs)
FunctionalPairs <- FunctionalPairs %>%
mutate(`Prevalence (%)` = ifelse(State == "Hypo", `Proportion of hypo (%)` , `Proportion of hyper (%)`)) %>%
select(Gene, ENSG, Probe, State, `Prevalence (%)`, `Fold change of gene expression`, Comparators,  `Raw.p`, `Adjusted.p`)
# Summary gene expression
summarized <- FunctionalPairs %>%
group_by(Gene) %>%
summarize( ENSG = first(ENSG),
State = first(State),
`Prevalence (%)` = mean(`Prevalence (%)`, na.rm = TRUE),
`Avg. fold change of gene expression` = mean(`Fold change of gene expression`, na.rm = TRUE)) %>%
arrange(desc(State), desc(`Prevalence (%)`),desc(`Avg. fold change of gene expression`))
write.csv(FunctionalPairs, paste0(OutputRoot, "/", CancerSite, ".csv"), row.names = FALSE)
write.csv(summarized, paste0(OutputRoot, "/", CancerSite, "_summary.csv"), row.names = FALSE)
