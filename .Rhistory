roadmap.epigenome.ids = "E096"  # "E096" indicates lung
# Third, run EpiMix
group.1 <-  "Cancer"
group.2 <- "Normal"
met.platform = "HM450"
EpiMixResults <- EpiMix(methylation.data = MET.data,
gene.expression.data = mRNA.data,
mode = mode,
sample.info = sample.info,
group.1 = group.1,
group.2 = group.2,
roadmap.epigenome.ids = roadmap.epigenome.ids,
met.platform = met.platform
)
